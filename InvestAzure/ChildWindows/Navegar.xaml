        <TextBox x:Name="txtName" HorizontalAlignment="Left" Height="23" Margin="122,101,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" TextChanged="txtName_TextChanged"/>
        <sdk:Label HorizontalAlignment="Left" Height="28" Margin="185,144,0,0" VerticalAlignment="Top" Width="120" Content="Fecha Ingreso"/>
        <TextBox x:Name="txtFecha" HorizontalAlignment="Left" Height="23" Margin="122,177,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244"/>
    </Grid>
</controls:ChildWindow>